package LA20002_1 is
   type Data is record
      B : Boolean := False;
   end record;

   procedure Do_It (D : in out Data);

end LA20002_1;

with LA20002_TC;
package body LA20002_1 is

   procedure Do_It (D : in out Data) is
   begin
      D.B := True;
      LA20002_TC.TC_Subprogram := LA20002_TC.FIRST_VERSION;
   end Do_It;

end LA20002_1;

with LA20002_1;
limited with LA20002_L;
procedure LA20002_0 (D : access LA20002_1.Data;
                     E : access LA20002_L.Extra_Info := null);

procedure LA20002_0 (D : access LA20002_1.Data;
                     E : access LA20002_L.Extra_Info := null) is
begin
    LA20002_1.Do_It (D.all);
end LA20002_0;

with Report;
use Report;
with LA20002_1;
with LA20002_0;
with LA20002_TC;
procedure LA200023 is
    -- Initial version.
    My_Data : aliased LA20002_1.Data;
begin
    Test ("LA20002","Check that an inconsistent partition may not be created");
    LA20002_0 (My_Data'Access);

    Failed ("Wrong version of main subprogram executed");

    Result;
end LA200023;
